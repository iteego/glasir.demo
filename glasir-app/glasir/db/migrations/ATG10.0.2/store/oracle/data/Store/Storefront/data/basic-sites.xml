<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template SYSTEM "dynamosystemresource:/atg/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
<import-items>
<add-item item-descriptor="siteConfiguration" id="basicSite">
  <set-property name="description"><![CDATA[ATG Basic Store]]></set-property>
  <set-property name="modificationTime"><![CDATA[01/01/2009 00:00:00]]></set-property>
  <set-property name="name"><![CDATA[ATG Basic Store]]></set-property>
  <set-property name="enabled"><![CDATA[true]]></set-property>

  <set-property name="defaultCatalog"><![CDATA[masterCatalog]]></set-property>
  <set-property name="defaultListPriceList"><![CDATA[listPrices]]></set-property>
  <set-property name="defaultSalePriceList"><![CDATA[salePrices]]></set-property>

  <set-property name="resourceBundle"><![CDATA[atg.projects.store.web.WebAppResources]]></set-property>
  <set-property name="newProductThresholdDays"><![CDATA[15]]></set-property>
  <set-property name="defaultPageSize"><![CDATA[12]]></set-property>

  <set-property name="cssFile"><![CDATA[/css/site/store]]></set-property>

  <set-property name="siteIcon"><![CDATA[/crsdocroot/images/storefront/atgStore_logo_small.png]]></set-property>
  <set-property name="largeSiteIcon"><![CDATA[/crsdocroot/images/atgstore/logo.png]]></set-property>

  <set-property name="defaultCountry"><![CDATA[US]]></set-property>

  <set-property name="billableCountries"><![CDATA[US]]></set-property>
  <set-property name="shippableCountries"><![CDATA[US]]></set-property>

  <set-property name="contextRoot"><![CDATA[/crs]]></set-property>
  <set-property name="productionURL"><![CDATA[/crs/basic]]></set-property>
</add-item>
</import-items>
</gsa-template>

